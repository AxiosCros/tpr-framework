{
  "name": "axios/tpr-framework",
  "description": "Design for API Development, Base on thinkphp 5.0.9 version",
  "keywords": [
    "framework",
    "ORM",
    "Api"
  ],
  "homepage": "https://github.com/AxiosCros/tpr-framework",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "axios",
      "email": "axioscros@aliyun.com"
    }
  ],
  "support": {
    "issues": "https://github.com/AxiosCros/tpr-framework/issues"
  },
  "require": {
    "php": ">=5.4",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-fileinfo": "*",
    "ext-gd": "*",
    "ext-iconv": "*",
    "axios/tpr-db": "^1.1"
  },
  "require-dev": {
    "mikey179/vfsStream": "~1.6",
    "phploc/phploc": "2.*",
